{
    "name": "mybit/laravel-google-distance-matrix",
    "description": "Laravel wrapper for the Google Distance Matrix API",
    "keywords": [
        "MyBit",
        "laravel",
        "laravel-google-distance-matrix"
    ],
    "homepage": "https://github.com/mybit/laravel-google-distance-matrix",
    "license": "MIT",
    "authors": [
        {
            "name": "Florian Heller",
            "email": "fh@my-b.it"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "^7.3",
        "illuminate/contracts": "^8.37",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Mybit\\GoogleDistanceMatrix\\": "src",
            "Mybit\\GoogleDistanceMatrix\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mybit\\GoogleDistanceMatrix\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mybit\\GoogleDistanceMatrix\\GoogleDistanceMatrixServiceProvider"
            ],
            "aliases": {
                "GoogleDistanceMatrix": "Mybit\\GoogleDistanceMatrix\\GoogleDistanceMatrixFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
